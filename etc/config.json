{
  "authEnabled": true,
  "baseHostname": "$S3_BRIDGE_HOST",
  "serverPort": 8080,
  "stripBucketPathFromAuth": false,
  "prettyPrint": true,
  "bucketPath": "~~/stor/s3_buckets",
  "baseSubdomain": "s3",
  "maxRequestBodySize": 1073741824,
  "s3Version": "2006-03-01",
  "accessKey": "$AWS_ACCESS_KEY_ID",
  "secretKey": "$AWS_SECRET_ACCESS_KEY",
  "maxAllowedSkewMilliseconds": "900000",
  "defaultDurability": 2,
  "maxFilenameLength": 1024,
  "storageClassMappingToDurability": {
    "STANDARD": 2,
    "STANDARD_IA": 2,
    "REDUCED_REDUNDANCY": 1,
    "GLACIER": 1
  },
  "durabilityMappingToStorageClass": {
    "2": "STANDARD",
    "1": "REDUCED_REDUNDANCY"
  },
  "manta": {
    "user": "$MANTA_USER",
    "subuser": "$MANTA_SUBUSER",
    "role": "$MANTA_ROLE",
    "insecure": "$MANTA_TLS_INSECURE",
    "keyId": "$MANTA_KEY_ID",
    "url": "$MANTA_URL",
    "privateKeyPath": "$MANTA_KEY_PATH",
    "connectTimeout": 4000
  }
}

